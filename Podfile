def common_pods
    pod 'Firebase'
    pod 'Firebase/Auth'
    pod 'Firebase/Storage'
    pod 'Firebase/Database'
    pod 'SVProgressHUD'
    pod 'ChameleonFramework'
end

target 'NewsCombined' do
    common_pods
end

target 'NewsCombinedTests' do
    common_pods
end
